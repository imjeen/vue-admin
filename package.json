  "main": "index.js",
  "scripts": {
    "clean": "rm -rf ./dist/",
    "clean:all": "rm -rf ./node_modules/ ./dist/",
    "_build": "npx -n='-r @babel/register' webpack --config './task/webpack.build.js' --progress",
    "_build:watch": "npx -n='-r @babel/register' webpack --config './task/webpack.build.js' -w",
    "_dll": "npx -n='-r @babel/register' webpack --config './task/webpack.dll.js' --progress",
    "build": "npm run clean && npm run _dll && npm run _build",
    "watch": "npm run clean && npm run _dll && npm run _build:watch",
    "watch:test": "DEPLOY_ENV=test npm run watch",
    "analyzer": "npm run clean && npm run _dll && npx -n '-r @babel/register' webpack --config './task/analyzer.js' --progress",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
